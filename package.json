{
  "name": "cca-mobile",
  "description": "Community Conserved Areas",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "android:build-prod": "ionic capacitor copy android && cd android && ./gradlew assembleRelease && cd .. && echo 'apk available at ./android/app/build/outputs/apk/release'",
    "android:build": "ionic capacitor copy android && cd android && ./gradlew assembleDebug && cd .. && echo 'apk available at ./android/app/build/outputs/apk/debug'",
    "android:live": "ionic capacitor run android -l --external",
    "android:run": "ionic capacitor run android",
    "android:post-install": "node -r esm ./scripts/update-after.js",
    "android": "npx cap run android",
    "build": "tsc && vite build",
    "dev": "vite",
    "ionic:build": "tsc && vite build",
    "ionic:serve": "vite",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src && prettier --write src/**/*",
    "resources": "capacitor-resources -p android -I -S",
    "start": "vite"
  },
  "dependencies": {
    "@capacitor/android": "^3.6.0",
    "@capacitor/app": "^1.1.1",
    "@capacitor/browser": "^1.0.7",
    "@capacitor/core": "^3.6.0",
    "@capacitor/geolocation": "^1.3.1",
    "@capacitor/haptics": "^1.1.4",
    "@capacitor/keyboard": "^1.2.3",
    "@capacitor/network": "^1.0.7",
    "@capacitor/status-bar": "^1.0.8",
    "@capacitor/storage": "^1.2.5",
    "@capacitor/toast": "^1.0.8",
    "@chakra-ui/icons": "^2.0.4",
    "@chakra-ui/react": "^2.2.4",
    "@codetrix-studio/capacitor-google-auth": "^3.1.4",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.9.6",
    "@ionic/react": "^6.1.14",
    "@ionic/react-router": "^6.1.14",
    "@react-google-maps/api": "^2.12.0",
    "@rehooks/online-status": "^1.1.2",
    "@tinymce/tinymce-react": "^4.1.0",
    "axios": "^0.27.2",
    "blueimp-load-image": "^5.16.0",
    "dayjs": "^1.11.3",
    "debounce-promise": "^3.1.2",
    "framer-motion": "^6.5.1",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "immer": "^9.0.15",
    "ionicons": "^6.0.2",
    "jwt-decode": "^3.1.2",
    "naksha-components-react": "^4.4.4",
    "nanoid": "^4.0.0",
    "prettier": "^2.7.1",
    "query-string": "^7.1.1",
    "quill": "^1.3.7",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "react-error-boundary": "^3.1.4",
    "react-gbus": "^1.1.2",
    "react-hook-form": "^7.33.1",
    "react-i18next": "^11.18.1",
    "react-phone-number-input": "^3.2.4",
    "react-quilljs": "^1.3.0",
    "react-router": "5",
    "react-router-dom": "5",
    "react-select": "^5.4.0",
    "typescript": "^4.7.4",
    "use-immer": "^0.7.0",
    "use-indexeddb": "^1.0.9",
    "xss": "^1.0.13",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@capacitor/cli": "^3.6.0",
    "@types/dom-mediacapture-record": "^1.0.11",
    "@types/jest": "^28.1.5",
    "@types/node": "^18.0.4",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vitejs/plugin-react": "^2.0.0",
    "capacitor-resources": "^2.0.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "esm": "^3.2.25",
    "vite": "^3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
